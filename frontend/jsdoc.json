{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.(js|jsx)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true
      },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }
  